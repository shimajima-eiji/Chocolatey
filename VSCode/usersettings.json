{

  "window.zoomLevel"  : 0,
  "files.associations": {
    "*.gs" : "javascript",
    "*.tsv": "tsv"
  },
  "extensions.ignoreRecommendations": false,
  "workbench.sideBar.location"      : "right",
  "saveAndRun"                      : {
      "commands": [
        {
          "match"      : "\\.py$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {
          "match"      : "\\.sh$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {  // ansible
          "match"      : "hosts$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {  // ansible
          "match"      : "inventory$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {  // ansible
          "match"      : "ssh_config$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {  // ansible
          "match"      : "\\.cnf$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {
          "match"      : "\\.ini$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {
          "match"      : "\\.conf$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {
          "match"      : "\\.csv$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {
          "match"      : "\\.tsv$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {
          "match"      : "\\.xml$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {
          "match"      : "\\.json$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {
          "match"      : "\\.yml$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {
          "match"      : "\\.yaml$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {
          "match"      : "\\.tml$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        },
        {
          "match"      : "\\.toml$",
          "cmd"        : "sh (path)/winsettings/VSCode/SaveandRun.sh $(wslpath '${file}')",
          "useShortcut": false
        }
      ]
  },
  "sync.gist"                        : "(gist id)",
  "files.trimTrailingWhitespace"     : true,
  "sync.askGistName"                 : true,
  "sync.autoDownload"                : true,
  "sync.quietSync"                   : true,
  "git.confirmSync"                  : false,
  "sync.autoUpload"                  : true,
  "git.autofetch"                    : true,
  "terminal.integrated.shell.windows": "C:\\Windows\\System32\\wsl.exe",
  "files.autoSave"                   : "off",
  "todo-tree.defaultHighlight"       : {
    "foreground": "green",
    "type"      : "none"
  },
  "todo-tree.customHighlight": {
    "TODO" : {},
    "FIXME": {}
  },
}